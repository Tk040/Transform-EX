{
  "id": "Test-hylba1",
  "name": "Transform EX",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "23.5.0",
      "data": {
        "apiVersion": 2,
        "loadEvent": "use",
        "enableMenuRecording": true
      }
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "command",
      "id": "transformEX",
      "label": {
